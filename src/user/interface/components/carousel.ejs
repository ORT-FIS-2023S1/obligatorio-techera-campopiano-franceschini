<% if(!dailyMenu){ %>
<%- include('./noContent') %>
<% }else{ %>
<div id="carouselExampleCaptions" class="carousel slide container" data-ride="carousel">
  <ol class="carousel-indicators">
    <li data-target="#carouselExampleCaptions" data-slide-to="0" class="active"></li>
    <li data-target="#carouselExampleCaptions" data-slide-to="1"></li>
    <li data-target="#carouselExampleCaptions" data-slide-to="2"></li>
  </ol>
  <div class="carousel-inner rounded">
    <div class="carousel-item active">
      <img src=<%- dailyMenu.breakfast.imageURL %> class="d-block w-100 img-carousel" alt=<%- dailyMenu.breakfast.name %> />
      <div class="carousel-caption d-none d-md-block">
        <h5><%- dailyMenu.breakfast.name %></h5>
        <p>Ingredientes: <%-`${dailyMenu.breakfast.ingredients.join(' ,')}`%> , ricas en <%-`${dailyMenu.breakfast.nutritionalInformation}` %>
      </div>
    </div>
    <div class="carousel-item">
      <img src=<%- dailyMenu.lunch.imageURL %> class="d-block w-100 img-carousel" alt=<%- dailyMenu.breakfast.name %> />
      <div class="carousel-caption d-none d-md-block">
        <h5><%- dailyMenu.lunch.name %></h5>
        <p>Ingredientes: <%-`${dailyMenu.lunch.ingredients.join(' ,')}`%> , ricas en <%-`${dailyMenu.breakfast.nutritionalInformation}` %>
      </div>
    </div>
    <div class="carousel-item">
      <img src=<%- dailyMenu.snack.imageURL %> class="d-block w-100 img-carousel" alt=<%- dailyMenu.breakfast.name %> />
      <div class="carousel-caption d-none d-md-block">
        <h5><%- dailyMenu.snack.name %></h5>
        <p>Ingredientes: <%-`${dailyMenu.snack.ingredients.join(' ,')}`%> , ricas en <%-`${dailyMenu.breakfast.nutritionalInformation}` %>
      </div>
    </div>
  </div>
  <button class="carousel-control-prev" type="button" data-target="#carouselExampleCaptions" data-slide="prev">
    <span class="carousel-control-prev-icon" aria-hidden="true"></span>
    <span class="sr-only">Previous</span>
  </button>
  <button class="carousel-control-next" type="button" data-target="#carouselExampleCaptions" data-slide="next">
    <span class="carousel-control-next-icon" aria-hidden="true"></span>
    <span class="sr-only">Next</span>
  </button>
</div>
<% } %>